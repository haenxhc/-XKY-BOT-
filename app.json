    "AUTO_REACT": {
      "description": "Réactions automatiques aux messages.",
      "required": false,
      "value": "false"
    },
    "ANTI_LINK": {
      "description": "Supprimer les liens dans les groupes.",
      "required": false,
      "value": "true"
    },
    "ANTI_BAD": {
      "description": "Filtrer les mots interdits.",
      "required": false,
      "value": "true"
    },
    "FAKE_RECORDING": {
      "description": "Activer le fake recording.",
      "required": false,
      "value": "false"
    },
